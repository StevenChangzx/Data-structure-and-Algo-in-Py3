# Greedy
class Solution:
    def canJump(self, nums: List[int]) -> bool:
        reach = 0
        for i, n in enumerate(nums):
            if i > reach:
                return False
            reach = max(reach, i + n)
        return True
        
  
# Greedy(from back
class Solution:
    def canJump(self, nums: List[int]) -> bool:
        goals=len(nums)-1
        
        for i in range(len(nums)-1,-1,-1):
            if i + nums[i] >= goal:
                goal=i
            reach = max(reach, i + n)
        return True if goal==0 else False
